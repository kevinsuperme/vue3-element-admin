{
  "name": "vue3-element-admin-backend",
  "version": "1.0.0",
  "description": "Backend API for Vue3 Element Admin",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && npm run build:copy",
    "build:copy": "xcopy src\\*.js dist\\ /Y /S /I 2>nul || cp -r src/*.js dist/ 2>/dev/null || echo 'Copy completed'",
    "start": "node dist/server.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "init:db": "node scripts/init-db-safe.js",
    "init:db:memory": "node scripts/init-db-safe.js --memory",
    "init:db:safe": "node scripts/init-db-safe.js",
    "init:db:prod": "NODE_ENV=production node scripts/init-db-safe.js",
    "reset:db": "node scripts/init-db-safe.js --reset",
    "setup": "node scripts/setup.js",
    "setup:quick": "npm install && npm run init:db:memory",
    "check:db": "node scripts/check-mongodb.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "jwt",
    "mongodb"
  ],
  "author": "Admin",
  "license": "MIT",
  "dependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/express-mongo-sanitize": "^1.3.2",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "ioredis": "^5.8.2",
    "jest": "^29.6.2",
    "mongodb-memory-server": "^10.2.3",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.6",
    "typescript": "^5.1.6"
  }
}
