# 前端环境配置示例文件
# 复制此文件为 .env.local 并根据实际情况修改配置

# 应用基础配置
VITE_APP_TITLE=Vue3 Element Admin
VITE_APP_BASE_API=http://localhost:3000/api

# 开发环境配置
VITE_ENV=development

# 是否启用Mock数据
# true: 使用mock数据 (默认)
# false: 连接真实后端API
VITE_USE_MOCK=false

# 上传文件API地址
VITE_UPLOAD_URL=http://localhost:3000/api/files/upload

# WebSocket地址（如果需要）
# VITE_WS_URL=ws://localhost:3000

# 是否在开发环境启用ESLint
VITE_USE_ESLINT=true

# 是否在构建时生成sourceMap
VITE_SOURCEMAP=false

# 生产环境配置
VITE_DROP_CONSOLE=true
VITE_DROP_DEBUGGER=true