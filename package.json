{
  "name": "vue3-element-admin",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "author": {
    "name": "Kevinsuperme",
    "email": "iphone.com@live.cn",
    "url": "https://github.com/Kevinsuperme"
  },
  "scripts": {
    "dev:test": "vite --mode dev_test",
    "dev:prod": "vite --mode dev_prod",
    "build:test": "vite build --mode build_test",
    "build:prod": "vite build --mode build_prod",
    "preview": "vite preview --port 4173",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "test": "node scripts/test-runner.js",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "cypress run",
    "test:e2e:dev": "cypress open",
    "test:ci": "node scripts/test-runner.js ci",
    "test:coverage": "node scripts/test-runner.js coverage",
    "test:watch": "node scripts/test-runner.js unit:watch",
    "test:all": "node scripts/test-runner.js all",
    "test:report": "vitest --reporter=verbose --outputFile=tests/results/test-report.json",
    "test:clean": "rm -rf tests/coverage tests/results node_modules/.cache",
    "quality:check": "node scripts/quality-check.js",
    "quality:frontend": "node -e \"const {runFrontendQualityChecks} = require('./scripts/quality-check.js'); runFrontendQualityChecks() && process.exit(0) || process.exit(1)\"",
    "quality:backend": "node -e \"const {runBackendQualityChecks} = require('./scripts/quality-check.js'); runBackendQualityChecks() && process.exit(0) || process.exit(1)\"",
    "quality:security": "node -e \"const {runSecurityChecks} = require('./scripts/quality-check.js'); runSecurityChecks() && process.exit(0) || process.exit(1)\""
  },
  "dependencies": {
    "axios": "^1.11.0",
    "clipboard": "^2.0.11",
    "driver.js": "^1.3.6",
    "dropzone": "^5.9.3",
    "echarts": "^5.6.0",
    "element-plus": "^2.11.5",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.6.2",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^8.2.0",
    "pinia": "^3.0.3",
    "sortablejs": "^1.15.6",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@pivanov/vite-plugin-svg-sprite": "^3.1.3",
    "@rushstack/eslint-patch": "^1.12.0",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node20": "^20.1.6",
    "@types/node": "^22.17.0",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^4.0.5",
    "@vitest/ui": "^4.0.5",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.10.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^27.0.1",
    "mockjs": "^1.1.0",
    "sass": "^1.89.2",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^6.3.7",
    "vite-plugin-inspect": "^11.3.2",
    "vite-plugin-mock": "^2.9.8",
    "vitest": "^4.0.5",
    "vue-tsc": "^3.0.4"
  }
}
